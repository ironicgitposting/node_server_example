{
  "extends": "airbnb",
  "plugins": [],
  "rules": {
    "indent": "off",
    "no-underscore-dangle": "warn",
    "consistent-return": ["warn", { "treatUndefinedAsUnspecified": false }],
    "no-unused-vars": [
      "warn",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ]
  }
}
